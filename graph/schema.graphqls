# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  username: String!
  password: Boolean!
  email: String!
}

type Query {
  users: [User!]!
}

input NewUser {
  username: String!
  password: String!
  email: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}